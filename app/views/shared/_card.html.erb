<div class="card-listing">
  <img src="cloudinary" />
  <div class="card-listing-infos">
    <div>
      <h2><%= listing.title %></h2>
      <p><%= listing.description %></p>
      <button type="button" class="btn btn-info"><%= link_to "Show", listing %></button>
    </div>
    <h2 class="card-listing-pricing">Rating: <%= listing.rating %></h2>
    <img src="cloudinary" class="card-listing-user avatar-bordered" />
  </div>
</div>
